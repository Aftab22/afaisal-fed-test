{
  "parser": "babel-eslint",
  "root": true,
  "extends": ["eslint-config-airbnb", "plugin:react/recommended", "prettier/react", "plugin:prettier/recommended"],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  }
}
